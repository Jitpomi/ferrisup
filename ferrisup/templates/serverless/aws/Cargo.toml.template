[package]
name = "{{project_name}}"
version = "0.1.0"
edition = "2021"
publish = false

[dependencies]
serde = { version = "1.0.228", features = ["derive"] }
serde_json = "1.0.145"
tokio = { version = "1.47", features = ["macros", "rt-multi-thread"] }
anyhow = "1.0"
tracing = "0.1.41"
tracing-subscriber = "0.3.20"

# AWS Lambda dependencies
lambda_runtime = "0.14.4"
aws_lambda_events = "0.18.0"

[dev-dependencies]
cargo-lambda = "1.8.6"
http = "1.3.1"
