{
  "name": "aws_serverless",
  "description": "A Rust serverless function template for AWS Lambda",
  "type": "binary",
  "files": [
    {
      "source": "src/main.rs",
      "target": "src/main.rs"
    },
    {
      "source": "template.yml",
      "target": "template.yml"
    },
    {
      "source": "Makefile",
      "target": "Makefile"
    }
  ],
  "dependencies": [
    "serde = { version = \"1.0\", features = [\"derive\"] }",
    "serde_json = \"1.0\"",
    "tokio = { version = \"1.36\", features = [\"macros\", \"rt-multi-thread\"] }",
    "anyhow = \"1.0\"",
    "tracing = \"0.1\"",
    "tracing-subscriber = \"0.3\"",
    "lambda_runtime = \"0.9\"",
    "aws_lambda_events = \"0.12\""
  ],
  "dev-dependencies": [
    "cargo-lambda = \"1.0\"",
    "http = \"0.2\""
  ]
}
