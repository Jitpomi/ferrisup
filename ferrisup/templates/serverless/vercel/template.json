{
  "name": "vercel_serverless",
  "description": "A Rust serverless function template for Vercel Functions",
  "type": "binary",
  "files": [
    {
      "source": "src/main.rs",
      "target": "src/main.rs"
    },
    {
      "source": "vercel.json",
      "target": "vercel.json"
    }
  ],
  "dependencies": [
    "serde = { version = \"1.0\", features = [\"derive\"] }",
    "serde_json = \"1.0\"",
    "tokio = { version = \"1.36\", features = [\"macros\", \"rt-multi-thread\"] }",
    "anyhow = \"1.0\"",
    "tracing = \"0.1\"",
    "tracing-subscriber = \"0.3\"",
    "vercel_runtime = \"1.1\""
  ],
  "dev-dependencies": [
    "http = \"0.2\""
  ]
}
