{
  "name": "Vercel Edge API",
  "description": "A Rust-powered API function for Vercel Edge Runtime",
  "category": "edge",
  "subcategory": "api-function",
  "type": "library",
  "dependencies": {
    "default": [
      "wasm-bindgen = \"0.2\"",
      "js-sys = \"0.3\"",
      "web-sys = { version = \"0.3\", features = [\"console\", \"Headers\", \"Request\", \"Response\", \"ResponseInit\"] }",
      "serde = { version = \"1.0\", features = [\"derive\"] }",
      "serde_json = \"1.0\"",
      "wasm-bindgen-futures = \"0.4\""
    ]
  },
  "next_steps": [
    "cd {{project_name}}",
    "Install wasm-pack: cargo install wasm-pack",
    "Build the WebAssembly module: wasm-pack build --target web",
    "Install Vercel CLI: npm install -g vercel",
    "For local development: vercel dev",
    "Deploy to Vercel: vercel deploy",
    "Test your API: curl https://<your-project>.vercel.app/api/info"
  ]
}
