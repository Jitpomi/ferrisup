{
  "name": "Static Website with WebAssembly",
  "description": "A static website with client-side WebAssembly for Netlify",
  "category": "edge",
  "subcategory": "static-site",
  "type": "library",
  "dependencies": {
    "default": [
      "wasm-bindgen = \"0.2\"",
      "js-sys = \"0.3\"",
      "web-sys = { version = \"0.3\", features = [\"console\", \"Window\", \"Document\", \"Element\", \"HtmlElement\", \"Node\", \"Text\", \"MouseEvent\", \"EventTarget\"] }",
      "serde = { version = \"1.0\", features = [\"derive\"] }",
      "serde-wasm-bindgen = \"0.6\"",
      "wasm-bindgen-futures = \"0.4\""
    ]
  },
  "next_steps": [
    "cd {{project_name}}",
    "Install wasm-pack: cargo install wasm-pack",
    "Build the WebAssembly module: wasm-pack build --target web",
    "Install Netlify CLI: npm install -g netlify-cli",
    "For local development: netlify dev",
    "Deploy to Netlify: netlify deploy --prod"
  ]
}
