{
  "name": "counter",
  "description": "Simple counter with reactive state management",
  "files": [
    {
      "source": "Cargo.toml",
      "target": "Cargo.toml"
    },
    {
      "source": "src/lib.rs",
      "target": "src/lib.rs"
    },
    {
      "source": "src/main.rs",
      "target": "src/main.rs"
    },
    {
      "source": "index.html",
      "target": "index.html"
    },
    {
      "source": "style.css",
      "target": "style.css"
    },
    {
      "source": "README.md",
      "target": "README.md"
    }
  ],
  "dependencies": {
    "default": [
      "leptos = { version = \"0.7.8\", features = [\"csr\"] }",
      "console_log = \"1.0\"",
      "log = \"0.4.22\"",
      "console_error_panic_hook = \"0.1.7\"",
      "gloo-timers = { version = \"0.3.0\", features = [\"futures\"] }"
    ]
  },
  "dev-dependencies": {
    "default": [
      "wasm-bindgen = \"0.2.93\"",
      "wasm-bindgen-test = \"0.3.42\"",
      "web-sys = \"0.3.70\""
    ]
  },
  "next_steps": [
    "cd {{project_name}}",
    "# Install required tools if you haven't already",
    "rustup target add wasm32-unknown-unknown",
    "cargo install trunk",
    "# Start the development server",
    "trunk serve --open"
  ]
}
