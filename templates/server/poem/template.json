{
  "name": "poem-server",
  "description": "A Rust web server using the Poem framework",
  "type": "binary",
  "files": [
    {
      "source": "main.rs",
      "target": "src/main.rs"
    }
  ],
  "dependencies": {
    "default": [
      "poem = \"2.0\"",
      "tokio = { version = \"1.36\", features = [\"full\"] }",
      "serde = { version = \"1.0\", features = [\"derive\"] }",
      "serde_json = \"1.0\"",
      "tracing = \"0.1\"",
      "tracing-subscriber = \"0.3\""
    ]
  },
  "dev-dependencies": {
    "default": [
      "reqwest = { version = \"0.11\", features = [\"json\"] }"
    ]
  }
}
