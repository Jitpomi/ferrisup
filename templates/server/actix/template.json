{
  "name": "actix-server",
  "description": "A high-performance Rust web server using the Actix Web framework",
  "type": "binary",
  "files": [
    {
      "source": "main.rs",
      "target": "src/main.rs"
    },
    {
      "source": "Cargo.toml.template",
      "target": "Cargo.toml"
    },
    {
      "source": "README.md",
      "target": "README.md"
    }
  ],
  "dependencies": {
    "default": [
      "actix-web = \"4.10.2\"",
      "serde = { version = \"1.0\", features = [\"derive\"] }",
      "serde_json = \"1.0\"",
      "log = \"0.4\"",
      "env_logger = \"0.10\""
    ]
  },
  "dev-dependencies": {
    "default": [
      "actix-rt = \"2.9\"",
      "reqwest = { version = \"0.11\", features = [\"json\"] }"
    ]
  },
  "next_steps": [
    "cd {{project_name}}",
    "cargo run",
    "# Test the API with: curl http://localhost:8080/",
    "# Add more routes in src/main.rs",
    "# Enable logging with: RUST_LOG=info cargo run"
  ]
}
