{
  "name": "server",
  "description": "A Rust web server template",
  "type": "binary",
  "options": [
    {
      "name": "server_framework",
      "description": "Server framework",
      "type": "select",
      "options": ["axum", "actix", "poem"],
      "default": "axum"
    }
  ],
  "files": [
    {
      "source": "README.md",
      "target": "README.md"
    }
  ],
  "redirect": {
    "axum": "server/axum",
    "actix": "server/actix",
    "poem": "server/poem"
  }
}
