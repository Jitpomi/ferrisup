{
  "name": "tauri-client",
  "description": "A desktop application using Tauri framework with Rust backend",
  "type": "binary",
  "files": [
    {
      "source": "main.rs",
      "target": "src/main.rs"
    }
  ],
  "dependencies": {
    "default": [
      "tauri = { version = \"1.5\", features = [\"api-all\"] }",
      "serde = { version = \"1.0\", features = [\"derive\"] }",
      "serde_json = \"1.0\""
    ]
  },
  "dev-dependencies": {
    "default": [
      "tauri-build = \"1.5\"",
      "wasm-bindgen = \"0.2\""
    ]
  }
}
