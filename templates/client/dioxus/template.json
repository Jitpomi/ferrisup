{
  "name": "dioxus-client",
  "description": "A modern Rust web application using Dioxus framework",
  "type": "binary",
  "files": [
    {
      "source": "main.rs",
      "target": "src/main.rs"
    }
  ],
  "dependencies": {
    "default": [
      "dioxus = \"0.5\"",
      "dioxus-web = \"0.5\"",
      "serde = { version = \"1.0\", features = [\"derive\"] }",
      "serde_json = \"1.0\""
    ]
  },
  "dev-dependencies": {
    "default": [
      "wasm-bindgen = \"0.2\""
    ]
  }
}
