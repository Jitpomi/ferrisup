{
  "name": "todo",
  "description": "Todo application with filtering capabilities",
  "files": [
    {
      "source": "Cargo.toml",
      "target": "Cargo.toml"
    },
    {
      "source": "src/lib.rs",
      "target": "src/lib.rs"
    },
    {
      "source": "src/main.rs",
      "target": "src/main.rs"
    },
    {
      "source": "index.html",
      "target": "index.html"
    },
    {
      "source": "style.css",
      "target": "style.css"
    }
  ],
  "dependencies": {
    "default": [
      "leptos = { version = \"0.7.8\", features = [\"csr\"] }",
      "console_log = \"1.0\"",
      "log = \"0.4.22\"",
      "console_error_panic_hook = \"0.1.7\"",
      "gloo-timers = { version = \"0.3.0\", features = [\"futures\"] }",
      "uuid = { version = \"1.6\", features = [\"v4\", \"js\"] }"
    ]
  },
  "dev-dependencies": {
    "default": [
      "wasm-bindgen = \"0.2.93\"",
      "wasm-bindgen-test = \"0.3.42\"",
      "web-sys = \"0.3.70\""
    ]
  }
}
