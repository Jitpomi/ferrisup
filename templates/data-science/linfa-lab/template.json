{
  "name": "data-science/linfa-lab",
  "description": "A Rust machine learning template using Linfa for classical ML algorithms (similar to scikit-learn in Python)",
  "type": "binary",
  "files": [
    {
      "source": "src/main.rs",
      "target": "src/main.rs"
    },
    {
      "source": "src/datasets.rs",
      "target": "src/datasets.rs"
    },
    {
      "source": "src/models.rs",
      "target": "src/models.rs"
    },
    {
      "source": "src/evaluation.rs",
      "target": "src/evaluation.rs"
    },
    {
      "source": "Cargo.toml.template",
      "target": "Cargo.toml"
    },
    {
      "source": "README.md",
      "target": "README.md"
    },
    {
      "source": "example_data.csv",
      "target": "data/example_data.csv"
    }
  ],
  "dependencies": {
    "default": [
      "linfa = \"0.7.1\"",
      "linfa-datasets = { version = \"0.7.1\", features = [\"iris\", \"diabetes\", \"winequality\"] }",
      "linfa-linear = \"0.7.1\"",
      "linfa-logistic = \"0.7.1\"",
      "linfa-trees = \"0.7.1\"",
      "linfa-clustering = \"0.7.1\"",
      "linfa-reduction = \"0.7.1\"",
      "linfa-svm = \"0.7.1\"",
      "linfa-nn = \"0.7.1\"",
      "linfa-preprocessing = \"0.7.1\"",
      "ndarray = { version = \"0.15\", features = [\"serde\"] }",
      "ndarray-rand = \"0.14\"",
      "ndarray-stats = \"0.5\"",
      "ndarray-csv = \"0.5\"",
      "csv = \"1.3\"",
      "plotters = \"0.3\"",
      "anyhow = \"1.0\"",
      "clap = { version = \"4.5\", features = [\"derive\"] }",
      "serde = { version = \"1.0\", features = [\"derive\"] }",
      "rand = \"0.8\"",
      "rand_distr = \"0.4\"",
      "indicatif = \"0.17\""
    ]
  },
  "next_steps": [
    "🧪 Try the example classification: cd {{project_name}} && cargo run -- classify",
    "📊 Run clustering analysis: cargo run -- cluster",
    "📉 Try dimensionality reduction: cargo run -- reduce",
    "🔍 Load your own dataset: cargo run -- custom -f data/your_data.csv",
    "📚 See all available commands: cargo run -- help"
  ]
}
