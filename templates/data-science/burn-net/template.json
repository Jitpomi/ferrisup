{
  "name": "data-science/burn-net",
  "description": "A Rust deep learning template using Burn for neural networks (similar to PyTorch in Python)",
  "type": "binary",
  "files": [
    {
      "source": "src/main.rs",
      "target": "src/main.rs"
    },
    {
      "source": "src/model.rs",
      "target": "src/model.rs"
    },
    {
      "source": "src/dataset.rs",
      "target": "src/dataset.rs"
    },
    {
      "source": "src/train.rs",
      "target": "src/train.rs"
    },
    {
      "source": "Cargo.toml.template",
      "target": "Cargo.toml"
    },
    {
      "source": "README.md",
      "target": "README.md"
    }
  ],
  "dependencies": {
    "default": [
      "burn = { version = \"0.16.0\", features = [\"train\", \"ndarray\", \"autodiff\", \"dataset\", \"vision\"] }",
      "burn-tensor = { version = \"0.16.0\", features = [\"ndarray\"] }",
      "burn-autodiff = \"0.16.0\"",
      "burn-train = \"0.16.0\"",
      "burn-dataset = \"0.16.0\"",
      "burn-ndarray = \"0.16.0\"",
      "ndarray = \"0.15\"",
      "anyhow = \"1.0\"",
      "clap = { version = \"4.5\", features = [\"derive\"] }",
      "serde = { version = \"1.0\", features = [\"derive\"] }",
      "serde_json = \"1.0\"",
      "rand = \"0.8\"",
      "indicatif = \"0.17\"",
      "image = \"0.24\""
    ]
  }
}
