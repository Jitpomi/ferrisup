{
  "name": "serverless",
  "description": "A Rust serverless function template for cloud deployment",
  "type": "binary",
  "files": [
    {
      "source": "main.rs",
      "target": "src/main.rs"
    },
    {
      "source": "Cargo.toml.template",
      "target": "Cargo.toml"
    }
  ],
  "database": {
    "db_type": "dynamodb",
    "orm": "aws-sdk"
  },
  "dependencies": {
    "default": [
      "serde = { version = \"1.0\", features = [\"derive\"] }",
      "serde_json = \"1.0\"",
      "tokio = { version = \"1.36\", features = [\"macros\", \"rt-multi-thread\"] }",
      "anyhow = \"1.0\"",
      "tracing = \"0.1\"",
      "tracing-subscriber = \"0.3\""
    ],
    "aws": [
      "lambda_runtime = \"0.9\"",
      "aws_lambda_events = \"0.12\""
    ],
    "gcp": [
      "cloud-functions = \"0.4\""
    ],
    "azure": [
      "azure_functions = \"0.46\""
    ],
    "vercel": [
      "vercel_runtime = \"1.1\""
    ],
    "netlify": [
      "netlify_lambda_http = \"0.5\""
    ]
  },
  "dev-dependencies": {
    "default": [
      "cargo-lambda = \"1.0\"",
      "http = \"0.2\""
    ]
  }
}
